{
  "id": "anthropic",
  "name": "Anthropic Claude",
  "description": "Integration with Anthropic Claude models",
  "website": "https://www.anthropic.com/",
  "hints": [
    {
      "key": "api_key",
      "description": "Your Anthropic API key",
      "required": true,
      "field_type": "string"
    },
    {
      "key": "model",
      "description": "The Claude model to use",
      "required": true,
      "field_type": "string"
    },
    {
      "key": "temperature",
      "description": "Controls randomness: 0 is deterministic, higher values make output more random",
      "required": false,
      "field_type": "number"
    },
    {
      "key": "max_tokens",
      "description": "The maximum number of tokens to generate",
      "required": false,
      "field_type": "number"
    },
    {
      "key": "top_p",
      "description": "Controls diversity via nucleus sampling",
      "required": false,
      "field_type": "number"
    },
    {
      "key": "top_k",
      "description": "Controls diversity by limiting to top k tokens",
      "required": false,
      "field_type": "number"
    }
  ],
  "requires_api_key": true,
  "api_key_name": "api_key",
  "model_name_key": "model",
  "example_models": [
    "claude-3-opus-20240229",
    "claude-3-sonnet-20240229",
    "claude-3-haiku-20240307",
    "claude-2.1",
    "claude-2.0"
  ]
}