{
  "$schema": "../../../../json_schema/model-schema.json",
  "id": "openrouter",
  "name": "OpenRouter",
  "description": "Integration with OpenRouter",
  "website": "https://openrouter.ai/",
  "type": "llm",
  "inference_type": ["chat"],
  "inference_fields": ["temperature", "top_p", "top_k", "reasoning"],
  "engine": "openrouter",
  "fields": [
    {
      "key": "api_key",
      "label": "API Key",
      "placeholder": "Your OpenRouter API key",
      "required": true,
      "field_type": "secret",
      "links": [
        {
          "label": "Get API Key",
          "url": "https://openrouter.ai/keys/"
        }
      ]
    },
    {
      "key": "model",
      "label": "Model",
      "placeholder": "The OpenRouter model to use",
      "required": true,
      "field_type": "string",
      "hints": [
        "gemini-2.5-pro-preview-03-25",
        "gemini-2.0-flash",
        "gemini-2.5-pro-exp-03-25",
        "gemini-2.0-flash-thinking-exp"
      ],
      "request": {
        "label": "Get Models",
        "method": "GET",
        "url": "https://openrouter.ai/api/v1/models",
        "headers": {
          "Authorization": "Bearer {api_key}"
        },
        "response": {
          "label": "name",
          "value": "id",
          "parse": {
            "data": {
              "type": "array",
              "items": {
                "type": "object",
                "id": "string",
                "name": "string"
              }
            }
          }
        }
      }
    },
    {
      "key": "base_url",
      "label": "Base URL",
      "placeholder": "The base URL of the OpenRouter API",
      "required": true,
      "default": "https://openrouter.ai/api/v1",
      "field_type": "url"
    }
  ]
}
